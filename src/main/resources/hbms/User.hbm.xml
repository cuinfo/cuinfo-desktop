<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class dynamic-insert="false" dynamic-update="false" mutable="true" name="org.cuinfo.pojo.User" optimistic-lock="version" polymorphism="implicit" select-before-update="false">
      <id name="id" type="integer">
          <column name="id" />
          <generator class="native" />
      </id>
      <property name="loginName"/>
      <property name="loginPsw"/>
      <property name="eMail"/>
      <property name="name"/>
      <property name="sex"/>

  </class>
</hibernate-mapping>
